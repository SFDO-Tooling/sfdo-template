[ignore]
.*/dist/.*
.*/test/.*
.*/venv/.*

[include]

[libs]

[lints]
all=warn
sketchy-null-string=off
sketchy-null-bool=off
deprecated-utility=off
dynamic-export=off

[options]
include_warnings=true
module.system.node.resolve_dirname=src/js
module.system.node.resolve_dirname=static
module.system.node.resolve_dirname=node_modules
server.max_workers=4

[strict]
